#*TWO SUM*# (15/02/26)
#include <stdio.h>

int main() {
    int nums[5] = {3,2,6,1,4};
    int target = 5;

    for (int i = 0; i < 5; i++) {
        for (int j = i + 1; j < 5; j++) {
            if (nums[i] + nums[j] == target) {
                printf("[%d, %d]\n", i, j);
                return 0;
            }
        }
    }
}
